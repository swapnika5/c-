#include<iostream.h>
#include<conio.h>
int non_recursive(int);
int main()
{
  int n,f;
  clrscr();
  cout<<"ENTER N VALUE:";
  cin>>n;
  f=non_recursive(n);
  cout<<"FACTORIAL is:"<<f;
  getch();
  return 0;
}
int non_recursive(int n)
{
  int a=1;
  while(n>0)
   {
     a=a*n;
     n--;
   }
  return a;
}
